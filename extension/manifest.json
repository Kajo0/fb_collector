{
  "manifest_version": 2,
  "name": "Friend collector",
  "description": "Collect friends",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Collect friends"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "all_frames": true,
    "js":      ["scripts/content.js"]
  }],
  "permissions": [
    "activeTab"
  ]
}
